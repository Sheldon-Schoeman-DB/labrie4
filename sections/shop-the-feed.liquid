{%- if settings.animate_content_show -%}
<link rel="stylesheet" href="{{ 'component-price.css' | asset_url }}" media="print" onload="this.media='all'">
<link rel="stylesheet" href="{{ 'component-product-card.css' | asset_url }}" media="print" onload="this.media='all'">
<noscript>{{ 'component-price.css' | asset_url | stylesheet_tag }}</noscript>
<noscript>{{ 'component-product-card.css' | asset_url | stylesheet_tag }}</noscript>
{%- else -%}
{{ 'component-price.css' | asset_url | stylesheet_tag }}
{{ 'component-product-card.css' | asset_url | stylesheet_tag }}
{%- endif -%}

{{ 'section-banners.css' | asset_url | stylesheet_tag }}
{{ 'component-video.css' | asset_url | stylesheet_tag }}
{{ 'component-grid-product.css' | asset_url | stylesheet_tag }}
{%- if section.settings.swipe_on_mobile or section.settings.desktop_slider contains "show" -%}
{{ 'component-slider.css' | asset_url | stylesheet_tag }}
{%- endif -%}

{%- render "section-background" section:section -%}

{%- capture section_styles -%}
{%- if section.settings.color_swipe_buttons  != "rgba(0,0,0,0)" -%}
#minion-section-{{ section.id }} .slider__button{
  --color-base:{{ section.settings.color_swipe_buttons }};
}
{%- endif -%}
{%- if section.settings.color_meta != "rgba(0,0,0,0)" -%}
#minion-section-{{ section.id }} .product-card__meta{
  --color-base:{{section.settings.color_meta}};
}
{%- endif -%}
{%- if section.settings.color_price  != "rgba(0,0,0,0)" -%}
#minion-section-{{ section.id }} .price__main-color,
#minion-section-{{ section.id }} .price__regular{
  color:{{ section.settings.color_price }};
}
{%- endif -%}
{%- if section.settings.color_save_price  != "rgba(0,0,0,0)" -%}
#minion-section-{{ section.id }} .price__sale{
  color:{{ section.settings.color_save_price }};
}
{%- endif -%}
{%- if section.settings.color_all_elements  != "rgba(0,0,0,0)" -%}
#minion-section-{{ section.id }} .product-card__container,
#minion-section-{{ section.id }} .unit-price{
  color:{{ section.settings.color_all_elements }};
}
#minion-section-{{ section.id }} .product-card__heading{
  --color-base:{{ section.settings.color_all_elements }};
}
{%- endif -%}
.shop-the-feed__media-buttons__button{
  --color-media-icon: {{ section.settings.social_media_icons }};
  {%- if section.settings.social_media_icons_hover  != "rgba(0,0,0,0)" -%}
  --color-base-custom: {{ section.settings.social_media_icons_hover }};
  {%- endif -%}
}
{%- if section.settings.color_button != "rgba(0,0,0,0)" -%}
{%- assign accent_brightness = section.settings.color_button | color_extract: 'lightness' -%}
#minion-section-{{ section.id }} .secondary-btn__container .btn{
  --background: {{ section.settings.color_button }};
  {%- if accent_brightness < 50 -%}
  --color: var(--auto-text-color-button-light);
  --fill: var(--auto-text-color-button-light);
  {%- else -%}
  --color: var(--auto-text-color-button-dark);
  --fill: var(--auto-text-color-button-dark);
  {%- endif -%}
}
{%- endif -%}

{%- if section.settings.slider_media_arrow != "rgba(0,0,0,0)" -%}
{%- assign accent_brightness = section.settings.slider_media_arrow | color_extract: 'lightness' -%}
#minion-section-{{ section.id }} [class*="slider-desktop__buttons__"] .btn{
  --background: {{ section.settings.slider_media_arrow }};
  {%- if accent_brightness < 50 -%}
  --fill: var(--auto-text-color-button-light);
  {%- else -%}
  --fill: var(--auto-text-color-button-dark);
  {%- endif -%}
}
{%- endif -%}
{%- if section.settings.slider_media_arrow_hover != "rgba(0,0,0,0)" -%}
{%- assign accent_brightness = section.settings.slider_media_arrow_hover | color_extract: 'lightness' -%}
#minion-section-{{ section.id }} [class*="slider-desktop__buttons__"] .btn:hover{
  --color-base: {{ section.settings.slider_media_arrow_hover }};
}
#minion-section-{{ section.id }} [class*=slider-desktop__buttons_]>.btn:hover svg path{
  {%- if accent_brightness < 50 -%}
  fill: var(--auto-text-color-button-light);
  {%- else -%}
  fill: var(--auto-text-color-button-dark);
  {%- endif -%}
}
{%- endif -%}
{%- if section.settings.color_play_button != "rgba(0,0,0,0)" -%}
{%- assign accent_brightness = section.settings.color_play_button | color_extract: 'lightness' -%}
#minion-section-{{ section.id }} .video-ctrl-banner .video-ctrl__play{
  --color-play-button-custom: {{ section.settings.color_play_button }};
  {%- if accent_brightness < 50 -%}
  --fill: var(--auto-text-color-button-light);
  {%- else -%}
  --fill: var(--auto-text-color-button-dark);
  {%- endif -%}
}
{%- endif -%}
{%- endcapture -%}
<style>{{ section_styles }}
.shop-the-feed__item{
  overflow: hidden;
  -webkit-backface-visibility: hidden;
  -moz-backface-visibility: hidden;
  -webkit-transform: translate3d(0, 0, 0);
  -moz-transform: translate3d(0, 0, 0);
  border-radius:var(--radius);
  position:relative;
}
.secondary-btn__container{
  display: flex;
  flex-wrap:wrap;
  justify-content:flex-start;
  gap:10px;
  width:100%;
}
.shop-the-feed__overlay{
  position:absolute;
  top:0;
  left:0;
  width:100%;
  height:100%;
  {%- if section.settings.color_overlay_gradient != blank -%}
  background:{{ section.settings.color_overlay_gradient }};
  {%- else -%}
  {%- if section.settings.color_overlay == "rgba(0,0,0,0)" -%}
  background: linear-gradient(180deg, rgb(from var(--color-base) r g b / 0) 69.78%,rgb(from var(--color-base) r g b / 100%));
  {%- else -%}
  background: linear-gradient(180deg, rgb(from {{ section.settings.color_overlay }} r g b / 0) 69.78%,rgb(from {{ section.settings.color_overlay }} r g b / 100%));
  {%- endif -%}
  {%- endif -%}
  z-index:0;
}
.shop-the-feed__media-buttons{
  display:flex;
  flex-direction:row;
  gap:0;
  align-items:flex-start;
  justify-content:flex-start;
  position:absolute;
  top:15px;
  right:15px;
  z-index:1;
}
.shop-the-feed__product{
  position:absolute;
  left:20px;
  right:20px;
  bottom:20px;
  z-index:1;
}
.shop-the-feed__media-buttons__button{
  display:flex;
  padding:5px;
}
.shop-the-feed__media-buttons__button svg path{
  fill:var(--color-media-icon);
}
.shop-the-feed__item [style*=--aspect-ratio].placeholder__svg--bg svg{
  width:auto;
}
@media (min-width:1025px){
  .shop-the-feed__media-buttons__button:hover svg path{
    fill:var(--color-base-custom, var(--color-base));
  }
}
@media (max-width:1024px){
  .shop-the-feed__media-buttons{
    top:10px;
    right:10px;
  }
  .shop-the-feed__media-buttons__button{
    padding:10px;
  }
}
</style>

{%- capture page_width -%}{%- if section.settings.width != "default" %}{{ section.settings.width }}{%- else -%}{{ settings.page_width_main }}{%- endif -%}{%- endcapture -%}
<div id="minion-section-{{ section.id }}" class="top-spacing-{{ section.settings.top_spacing }}{% if section.settings.background != 'hide' and section.settings.background_width == "fullwidth" %} section-background section-background--{{ section.settings.background_spacing }}{% endif %}"
     data-section-name="shop-the-feed">
  <div class="{{ page_width }} mb--last-child-0">
    {% if section.settings.background != 'hide' and section.settings.background_width == "content_width" %}<div class="section-background section-background--{{ section.settings.background_spacing }} border__radius mb--last-child-0">{%- endif -%}
    
    {%- render "section-heading" -%}

    <div class="shop-the-feed-items featured-collection-items{% if section.settings.desktop_slider == "show_mask" %} slider-desktop__mask{% endif %}">
      {%- if section.settings.desktop_slider contains "show" -%}
      <div class="slider-desktop__buttons__prev slider-desktop__buttons__hide">
        <div class="btn btn_zoom">{%- render "icon-arrow-left" -%}</div>
      </div>
      <div class="slider-desktop__buttons__next slider-desktop__buttons__hide">
        <div class="btn btn_zoom">{%- render "icon-arrow-right" -%}</div>
      </div>
      {%- endif -%}
      <slider-component class="grid el_mt_40 {{ section.settings.texts_aligment }}{% if section.settings.desktop_slider contains "show" %} slider-desktop{% if section.settings.desktop_slider_scroll_one %} slider-desktop--scroll-one{% endif %}{% endif %}{% if section.settings.swipe_on_mobile %} slider slider--phone{% endif %}{% if section.settings.heading_aligment == "text-center" %} justify-content-center--no-phone{% endif %}{% if section.settings.heading_aligment == "text-center" %} justify-content-center--no-phone{% endif %}">
        {%- for block in section.blocks -%}
        <div class="col-sp-12 {{ section.settings.grid_tablet }} {{ section.settings.grid }}{% if section.settings.swipe_on_mobile %} slider__slide{% endif %}">
          {%- render "shop-the-feed-item" section:section, block:block -%}
        </div>
        {%- endfor -%}
      </slider-component>
    </div>

    {%- liquid
        assign devider = 1.0
        assign at_most = section.blocks | size
    -%}
    {%- assign length = at_most | divided_by: devider | ceil -%}
    {%- if section.settings.swipe_on_mobile and length > 1 -%}
    <div class="mt40 grid slider__buttons slider--phone">
      <div class="col-12">
        {%- liquid
            capture alignment
            if section.settings.heading_aligment == "text-center"
            echo "justify-content-center"
            endif
          	endcapture
          	render "slider-buttons", alignment:alignment, length:length
        -%}
      </div>
    </div>
    {%- endif -%}
    

    {%- liquid
        assign btn_1 = false
        assign btn_2 = false
        assign i_n_1 = section.settings.icon_name
        assign i_n_2 = section.settings.icon_name_2
        if section.settings.link_name != blank or i_n_1 != blank
        assign btn_1 = true
        endif
        if section.settings.link_name_2 != blank or i_n_2 != blank
        assign btn_2 = true
        endif
    -%}
    {%- if btn_1 or btn_2 -%}
    <div class="grid self_mt_60">
      <div class="secondary-btn__container{% if section.settings.heading_aligment == "text-center" %} justify-content-center{% endif %}">
        {%- if btn_1 -%}
        <a href="{{ section.settings.url }}" class="btn btn_wide btn_zoom{% if settings.enable_btn_with_arrow %} btn__animation-arrow{% endif %} w-full-sp"{% if section.settings.url == blank %} aria-disabled="true"{% endif %}>
          {%- if i_n_1 != blank -%}{%- render "icons-list-social" icon_name: i_n_1 -%}{%- endif -%}<span>{{ section.settings.link_name }}</span>
        </a>
        {%- endif -%}
        {%- if btn_2 -%}
        <a href="{{ section.settings.url_2 }}" class="btn btn_wide btn_zoom{% if settings.enable_btn_with_arrow %} btn__animation-arrow{% endif %} w-full-sp"{% if section.settings.url_2 == blank %} aria-disabled="true"{% endif %}>
          {%- if i_n_2 != blank -%}{%- render "icons-list-social" icon_name: i_n_2 -%}{%- endif -%}<span>{{ section.settings.link_name_2 }}</span>
        </a>
        {%- endif -%}
      </div>
    </div>
    {%- endif -%}

    {% if section.settings.background != 'hide' and section.settings.background_width == "content_width" %}</div>{%- endif -%}
  </div>
</div>

{%- if section.settings.button_type_product != 'hide' or section.settings.show_button_quickview -%}
<script src="{{ 'quick-add.min.js' | asset_url }}" defer="defer"></script>
{%- endif -%}

