{{ 'section-featured-collection-tabs.css' | asset_url | stylesheet_tag }}
{%- if settings.animate_content_show -%}
<link rel="stylesheet" href="{{ 'component-price.css' | asset_url }}" media="print" onload="this.media='all'">
<link rel="stylesheet" href="{{ 'component-product-card.css' | asset_url }}" media="print" onload="this.media='all'">
<noscript>{{ 'component-price.css' | asset_url | stylesheet_tag }}</noscript>
<noscript>{{ 'component-product-card.css' | asset_url | stylesheet_tag }}</noscript>
{%- if settings.swatches_design != 'none' and settings.enable_color_swatches -%}
<link rel="stylesheet" href="{{ 'product-card-variants.css' | asset_url }}" media="print" onload="this.media='all'">
<noscript>{{ 'product-card-variants.css' | asset_url | stylesheet_tag }}</noscript>
{%- endif -%}
{%- if section.settings.swipe_on_mobile or section.settings.desktop_slider contains "show" -%}
<link rel="stylesheet" href="{{ 'component-slider.css' | asset_url }}" media="print" onload="this.media='all'">
<noscript>{{ 'component-slider.css' | asset_url | stylesheet_tag }}</noscript>
{%- endif -%}
{%- else -%}
{{ 'component-price.css' | asset_url | stylesheet_tag }}
{{ 'component-product-card.css' | asset_url | stylesheet_tag }}
{%- if settings.swatches_design != 'none' and settings.enable_color_swatches -%}
{{ 'product-card-variants.css' | asset_url | stylesheet_tag }}
{%- endif -%}
{%- if section.settings.swipe_on_mobile or section.settings.desktop_slider contains "show" -%}
{{ 'component-slider.css' | asset_url | stylesheet_tag }}
{%- endif -%}
{%- endif -%}

{%- render "section-background" section:section -%}

{%- capture section_styles -%}
{%- if section.settings.color_meta != "rgba(0,0,0,0)" -%}
#minion-section-{{ section.id }} .product-card__meta{
  --color-base:{{section.settings.color_meta}};
}
{%- endif -%}
{%- if section.settings.color_button_quickview != "rgba(0,0,0,0)" -%}
{%- assign accent_brightness = section.settings.color_button_quickview | color_extract: 'lightness' -%}
#minion-section-{{ section.id }} .btn.btn-quickview{
  --background: {{ section.settings.color_button_quickview }};
  {%- if accent_brightness < 50 -%}
  --color: var(--auto-text-color-button-light);
  --fill: var(--auto-text-color-button-light);
  {%- else -%}
  --color: var(--auto-text-color-button-dark);
  --fill: var(--auto-text-color-button-dark);
  {%- endif -%}
}
{%- endif -%}
{%- if section.settings.color_button_product != "rgba(0,0,0,0)" -%}
{%- assign accent_brightness = section.settings.color_button_product | color_extract: 'lightness' -%}
#minion-section-{{ section.id }} .btn{
  --background: {{ section.settings.color_button_product }};
  {%- if accent_brightness < 50 -%}
  --color: var(--auto-text-color-button-light);
  --fill: var(--auto-text-color-button-light);
  {%- else -%}
  --color: var(--auto-text-color-button-dark);
  --fill: var(--auto-text-color-button-dark);
  {%- endif -%}
}
{%- endif -%}
{%- if section.settings.color_price  != "rgba(0,0,0,0)" -%}
#minion-section-{{ section.id }} .price__main-color,
#minion-section-{{ section.id }} .price__regular{
  color:{{ section.settings.color_price }};
}
{%- endif -%}
{%- if section.settings.color_save_price  != "rgba(0,0,0,0)" -%}
#minion-section-{{ section.id }} .price__sale{
  color:{{ section.settings.color_save_price }};
}
{%- endif -%}
{%- if section.settings.color_swipe_buttons  != "rgba(0,0,0,0)" -%}
#minion-section-{{ section.id }} .slider__button{
  --color-base:{{ section.settings.color_swipe_buttons }};
}
{%- endif -%}
{%- if section.settings.color_all_elements  != "rgba(0,0,0,0)" -%}
#minion-section-{{ section.id }} .product-card__container,
#minion-section-{{ section.id }} .product-card__variant-count__value,
#minion-section-{{ section.id }} .unit-price{
  color:{{ section.settings.color_all_elements }};
}
#minion-section-{{ section.id }} .product-card__heading,
#minion-section-{{ section.id }} .product-card__variant-item{
  --color-base:{{ section.settings.color_all_elements }};
}
#minion-section-{{ section.id }} .btn-small:before,
#minion-section-{{ section.id }} .btn-medium:before,
#minion-section-{{ section.id }} .btn-medium--icon>span:before {
  --color-accent:{{ section.settings.color_all_elements }};
}
#minion-section-{{ section.id }} .product-card__variant-item__button{
  --color-base:{{ section.settings.color_all_elements }};
  border-color:{{ section.settings.color_all_elements | color_modify: 'alpha', 0.3 }};
}
{%- endif -%}
{%- if section.settings.color_heading  != "rgba(0,0,0,0)" -%}
  #minion-section-{{ section.id }} .featured-collection-tabs__container__content{
    --color-base:{{ section.settings.color_heading }};
  }
{%- endif -%}
{%- if section.settings.color_border_1  != "rgba(0,0,0,0)" -%}
#minion-section-{{ section.id }} .product-card__variant-item:before{
  --color-base:{{ section.settings.color_border_1 }};
}
{%- endif -%}
{%- if section.settings.color_border_2  != "rgba(0,0,0,0)" -%}
#minion-section-{{ section.id }} .product-card__variant-item:after{
  --color-body-background:{{ section.settings.color_border_2 }};
}
{%- endif -%}
{%- endcapture -%}
{%- if section_styles != blank -%}
<style>{{ section_styles }}</style>
{%- endif -%}

<div id="minion-section-{{ section.id }}" class="top-spacing-{{ section.settings.top_spacing }}{% if section.settings.background != 'hide' and section.settings.background_width == "fullwidth" %} section-background section-background--{{ section.settings.background_spacing }}{% endif %}"
     data-section-name="featured-collection-tabs">
  <div class="{% if section.settings.width != "default" %}{{ section.settings.width }}{% else %}{{ settings.page_width_main }}{% endif %} mb--last-child-0">
    {% if section.settings.background != 'hide' and section.settings.background_width == "content_width" %}<div class="section-background section-background--{{ section.settings.background_spacing }} border__radius mb--last-child-0">{%- endif -%}
      
    {%- liquid
      if section.settings.color_subheading != "rgba(0,0,0,0)"
      assign color_subheading = ' style="--color-subheading:' | append: section.settings.color_subheading | append: ';"'
      endif
    -%}

    {%- if section.settings.subheading != blank -%}
    <div class="grid{% if section.settings.heading_aligment == "text-center" %} justify-content-center{% endif %}">
      <div class="col-st-12 col-6 {{ section.settings.heading_aligment }}">
        <div class="subheading h5"{{ color_subheading }}>{{ section.settings.subheading }}</div>
      </div>
    </div>
    {%- endif -%}
    <featured-collection-tabs class="grid heading--mb featured-collection-tabs{% if section.settings.heading_aligment == "text-center" %} featured-collection-tabs--center{% endif %}"
                              data-settings="{{ section.settings.texts_aligment }}___{{ section.settings.swipe_on_mobile }}___{% if section.settings.width != "default" %}{{ section.settings.width }}{% else %}{{ settings.page_width_main }}{% endif %}___{{ section.settings.grid | replace: ' ', '--' }}___{{ section.settings.mobile_grid }}___{{ section.settings.image_ratio | replace: ', ', '--' }}___{{ section.settings.show_button_quickview }}___{{ section.settings.show_badge_sold_out }}___{{ section.settings.show_badge_on_sale }}___{{ section.settings.show_badge_feature }}___{{ section.settings.show_secondary_image }}___{{ section.settings.show_vendor }}___{{ section.settings.product_name_size }}___{{ section.settings.hover_underline }}___{{ section.settings.truncate_product_name }}___{{ section.settings.show_price }}___{{ section.id }}___{{ section.settings.heading_aligment }}___{{ section.settings.product_limit }}___false___{{ section.settings.button_type_product }}___{{ section.settings.show_type }}___{{ section.settings.button_name }}___{{ section.settings.desktop_slider }}___{{ section.settings.desktop_slider_scroll_one }}___{{ section.settings.image_zoom_internal }}">
      <div class="col-st-12">
        <div class="featured-collection-tabs__container">
          <div class="featured-collection-tabs__container__content">
            {%- for block in section.blocks -%}
            <div class="featured-collection-tabs__item featured-collection-tabs__item-main h2 {% if forloop.first %} animation-underline--active{% endif %}" data-index="{{ forloop.index }}" tabindex="0" role="tab">{{ block.settings.heading_text }}</div>
            {%- endfor -%}
            <div class="tabs__indicator"></div>
          </div>
        </div>

        {%- for block in section.blocks -%}
        {%- capture items -%}
        {%- assign count = 0 -%}
        {%- for i in (1..12) -%}
        {%- assign collection_obj = "collection_" | append: i -%}
        {%- assign collection_obj = block.settings[collection_obj] -%}
        {%- assign collection_name = "collection_name_" | append: i -%}
        {%- assign collection_name = block.settings[collection_name] -%}
        {%- if collection_obj != blank -%}
        {%- assign count = count | plus: 1 -%}
        <div class="featured-collection-tabs__item featured-collection-tabs__item-secondary h4{% if forloop.first %} animation-underline--active{% endif %}"
             data-handle="{{ collection_obj.handle }}"
             data-placeholder="false"
             tabindex="0"
             role="tab">{% if collection_name == blank %}{{ collection_obj.title }}{% else %}{{ collection_name }}{% endif %}</div>
        {%- endif -%}
        {%- endfor -%}
        {%- endcapture -%}
        {%- if items != blank -%}
        <div class="featured-collection-tabs__container featured-collection-tabs__container-secondary{% unless forloop.first %} hide{% endunless %}{% if count <= 1 %} tab__hide-always{% endif %}" data-items-index="{{ forloop.index }}">
          <div class="featured-collection-tabs__container__content">{{ items }}</div>
        </div>
        {%- else -%}
        <div class="featured-collection-tabs__container featured-collection-tabs__container-secondary{% unless forloop.first %} hide{% endunless %} tab__hide-always" data-items-index="{{ forloop.index }}">
          <div class="featured-collection-tabs__item featured-collection-tabs__item-secondary h4 animation-underline--active"
               data-handle="all" data-placeholder="true"></div>
        </div>
        {%- endif -%}
        {%- endfor -%}
      </div>
    </featured-collection-tabs>
    <featured-collection-tabs-content class="display-block"></featured-collection-tabs-content>

    {% if section.settings.background != 'hide' and section.settings.background_width == "content_width" %}</div>{%- endif -%}
  </div>
</div>

{%- if section.settings.button_type_product != 'hide' or section.settings.show_button_quickview -%}
<script src="{{ 'quick-add.min.js' | asset_url }}" defer="defer"></script>
{%- endif -%}

